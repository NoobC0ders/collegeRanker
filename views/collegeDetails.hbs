    <!-- The Modal -->
    <div id="myModal" class="modal">

        <!-- Modal content -->
        <div class="modal-content">
            <span onclick="closeMOdal()" class="close">&times;</span>
            <form action="/login?backto=collegeDetails" method="POST">
                <p> Email: </p>
                <input class="modalEmail" required type="email" placeholder="yourname@email.com" name="email" id="">

                <button type="submit" class="modalsigninBtn">Sign In</button>
            </form>
        </div>

    </div>


    {{#if registration}}

    <div id="regModal" class="regModal">

        <div class="regModal-content">
            <p>
            <h2> Please Sign up! </h2>
            </p>
            <span onclick="closeregModal()" id="regclose" class="regclose">&times;</span>
            <form action="/register?backto=collegeDetails" method="POST">
                <p> Full Name: </p>
                <input class="modalEmail" type="text" name="name" id="name" placeholder="your name" required>
                <p> Email: </p>
                <input class="modalEmail" type="email" name="email" id="" value={{email}} readonly>
                <p> Mobile: </p>
                <input class="modalEmail" type="number" name="phone" id="mobile" placeholder="+91"
                    oninput="checkLength(this)" required>
                <p class="error" id="mobileErr"></p>
                <p> Address: </p>
                <textarea class="modalAddress" type="address" name="address" placeholder="Address" required></textarea>


                <button type="submit" id="regSubmit" class="regModalSubmitBtn">Register</button>
            </form>

        </div>

    </div>
    {{/if}}

    <script src="./notiflix/dist/notiflix-aio-2.3.3.min.js"></script>
